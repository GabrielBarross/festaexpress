{
  "header": {
    "blocks": [
      "header-layout.desktop",
      "header-layout.mobile"
    ]
  },
  "header.full": {
    "blocks": [
      "header-layout.desktop",
      "header-layout.mobile"
    ]
  },
  "header-layout.desktop": {
    "children": [
      "header-row#1-desktop",
      "header-row#2-desktop",
      "header-row#3-desktop"
    ]
  },

  "header-row#1-desktop": {
    "props":{
      "inverted":"true"
    },
    "children": [
      "login",
      "rich-text#orders",
      "rich-text#account",
      "header-spacer",
      "rich-text#text-top",
      "header-spacer",
      "vtex.menu@2.x:menu#websites"
    ]
  },
  "rich-text#orders": {
    "props": {
      "text": "[Meus Pedidos](/account#/orders)"
    }
  },
  "rich-text#account": {
    "props": {
      "text": "[Meu Cadastro](/account)"
    }
  },
  "rich-text#text-top": {
    "props": {
      "text": "Festa Express, desde 2015 fazendo sua festa"
    }
  },
  "header-row#2-desktop": {
    "children": [
      "header-spacer",
      "logo#desktop",
      "header-spacer",
      "vtex.menu@2.x:menu#category-menu",
      "header-spacer"
    ],
    "props": {
      "sticky": true
    }
  },
  
  "rich-text#teltop": {
    "props": {
      "text": "Televendas<br>(11) 3312-6600",
      "blockClass": "text-tel"
    }
  },
  "header-row#3-desktop": {
    "children": [
      "search-bar",
      "minicart",
      "rich-text#teltop"
    ],
    "props": {
      "inverted":true,
      "sticky": true,
      "blockClass":"search-top"
    }
  },
  "logo#desktop": {
    "props": {
      "title": "Festa Express",
      "href": "/",
      "url": "https://festaexpress.vteximg.com.br/assets/vtex.file-manager-graphql/images/logo___6151bcfe416bd248d57e425384334e5a.png",
      "width": "180"
    }
  },

  "minicart": {
    "blocks": [
      "product-summary"
    ],
    "props": {
      "type": "sidebar",
      "showRemoveButton": true,
      "showDiscount": true,
      "showSku": true,
      "labelMiniCartEmpty": "",
      "labelButtonFinishShopping": "IR PARA O CARRINHO",
      "enableQuantitySelector": true,
      "maxQuantity": 20,
      "labelClasses": "gray"
    }
  },

  "header-layout.mobile": {
    "children": [
      "header-row#1-mobile",
      "header-row#2-mobile"
    ]
  },
  "header-row#1-mobile": {
    "children": [
      "drawer",
      "logo#mobile",
      "header-spacer",
      "login",
      "minicart"
    ],
    "props": {
      "sticky": true,
      "blockClass": "main-header-mobile"
    }
  },
  "drawer": {
    "children": [
      "vtex.menu@2.x:menu#category-menu-mobile"
    ]
  },
  "logo#mobile": {
    "props": {
      "title": "Festa Express",
      "href": "/",
      "url": "https://festaexpress.vteximg.com.br/assets/vtex.file-manager-graphql/images/logo___6151bcfe416bd248d57e425384334e5a.png",
      "width": "70"
    }
  },

  "header-row#2-mobile": {
    "children": [
      "search-bar"
    ],
    "props": {
      "sticky": true
    }
  }
}