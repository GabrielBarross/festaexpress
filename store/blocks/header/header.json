{
  "header": {
    "blocks": [
      "header-layout.desktop",
      "header-layout.mobile"
    ]
  },
  "header.full": {
    "blocks": [
      "header-layout.desktop",
      "header-layout.mobile"
    ]
  },
  "header-layout.desktop": {
    "children": [
      "header-row#1-desktop",
      "header-row#2-desktop",
      "header-row#3-desktop"
    ]
  },

  "header-row#1-desktop": {
    "props":{
      "inverted":"true",
      "blockClass":"topbar"
    },
    "children": [
      "login",
      "rich-text#orders",
      "rich-text#account",
      "header-spacer",
      "rich-text#text-top",
      "header-spacer",
      "rich-text#header-instagram",
      "rich-text#header-facebook",
      "rich-text#header-pintrest",
      "rich-text#header-blog",
      "rich-text#header-youtube",
      "rich-text#header-twitter"
    ]
  },
  "rich-text#header-instagram": {
    "props": {
      "text": "[![Instagram](https://festaexpress.vteximg.com.br/arquivos/header-instagram.png?v02 'Instagram')](https://www.instagram.com/festaexpress/)",
      "blockClass": "header-social"
    }
  },
  "rich-text#header-facebook": {
    "props": {
      "text": "[![Facebook](https://festaexpress.vteximg.com.br/arquivos/header-facebook.png?v01 'Facebook')](https://www.facebook.com/FestaExpress)",
      "blockClass": "header-social"
    }
  },
  "rich-text#header-pintrest": {
    "props": {
      "text": "[![Pintrest](https://festaexpress.vteximg.com.br/arquivos/header-pinterest.png?v01 'Pintrest')](https://br.pinterest.com/festaexpress)",
      "blockClass": "header-social"
    }
  },
  "rich-text#header-blog": {
    "props": {
      "text": "[![Blog](https://festaexpress.vteximg.com.br/arquivos/header-blog.png?v02 'Blog')](https://blog.festaexpress.com.br/)",
      "blockClass": "header-social"
    }
  },
  "rich-text#header-youtube": {
    "props": {
      "text": "[![Youtube](https://festaexpress.vteximg.com.br/arquivos/header-youtube.png?v01 'Youtube')](https://www.youtube.com/user/FestaExpress)",
      "blockClass": "header-social"
    }
  },
  "rich-text#header-twitter": {
    "props": {
      "text": "[![Twitter](https://festaexpress.vteximg.com.br/arquivos/header-twitter.png?v01 'Twitter')](https://twitter.com/festaexpress)",
      "blockClass": "header-social"
    }
  },
  "rich-text#orders": {
    "props": {
      "text": "[Meus Pedidos](/account#/orders)"
    }
  },
  "rich-text#account": {
    "props": {
      "text": "[Meu Cadastro](/account)"
    }
  },
  "rich-text#text-top": {
    "props": {
      "text": "Festa Express, desde 2015 fazendo sua festa"
    }
  },
  "header-row#2-desktop": {
    "children": [
      "logo#desktop",
      "vtex.menu@2.x:menu#category-menu"
    ],
    "props": {
      "sticky": true
    }
  },
  "header-row#3-desktop": {
    "children": [
      "search-bar",
      "minicart",
      "rich-text#teltop",
      "rich-text#loja-fisica"
    ],
    "props": {
      "inverted":true,
      "sticky": true,
      "blockClass":"search-top"
    }
  },
  "rich-text#teltop": {
    "props": {
      "text": "Televendas<br>(11) 3312-6600",
      "blockClass": "text-tel"
    }
  },
  "rich-text#loja-fisica": {
    "props": {
      "text": "[Loja FÃ­sica](/lojafisica)",
      "blockClass": "loja-fisica"
    }
  },
  "logo#desktop": {
    "props": {
      "title": "Festa Express",
      "href": "/",
      "url": "https://festaexpress.vteximg.com.br/assets/vtex.file-manager-graphql/images/logo___6151bcfe416bd248d57e425384334e5a.png",
      "width": "180"
    }
  },

  "minicart": {
    "blocks": [
      "product-summary"
    ],
    "props": {
      "type": "sidebar",
      "showRemoveButton": true,
      "showDiscount": true,
      "showSku": true,
      "labelMiniCartEmpty": "",
      "labelButtonFinishShopping": "IR PARA O CARRINHO",
      "enableQuantitySelector": true,
      "maxQuantity": 20,
      "labelClasses": "gray"
    }
  },

  "header-layout.mobile": {
    "children": [
      "header-row#1-mobile",
      "header-row#2-mobile",
      "vtex.menu@2.x:menu#category-menu"
    ]
  },
  "header-row#mobile-menu-icon": {
    "children": [
      "vtex.menu@2.x:menu#category-menu"
    ],
    "props": {
      "sticky": true,
      "blockClass": "main-header-mobile"
    }
  },
  "header-row#1-mobile": {
    "children": [
      "drawer",
      "logo#mobile",
      "header-spacer",
      "login",
      "minicart"
    ],
    "props": {
      "sticky": true,
      "blockClass": "main-header-mobile"
    }
  },
  "drawer": {
    "children": [
      "vtex.menu@2.x:menu#category-menu-mobile"
    ]
  },
  "logo#mobile": {
    "props": {
      "title": "Festa Express",
      "href": "/",
      "url": "https://festaexpress.vteximg.com.br/assets/vtex.file-manager-graphql/images/logo___6151bcfe416bd248d57e425384334e5a.png",
      "width": "70"
    }
  },

  "header-row#2-mobile": {
    "children": [
      "search-bar"
    ],
    "props": {
      "sticky": true
    }
  }
}