{
  "store.product": {
    "children": [
      "flex-layout.row#product-breadcrumb",
      "flex-layout.row#product-main",
      "shelf.relatedProducts"
    ]
  },
  "flex-layout.row#product-breadcrumb": {
    "props": {
      "marginTop": 4
    },
    "children": [
      "breadcrumb"
    ]
  },
  "flex-layout.col#spacer": {
    "props": {
      "width": "15%"
    }
  },
  "flex-layout.row#product-main": {
    "props": {
      "colGap": 7,
      "rowGap": 7,
      "marginTop": 4,
      "marginBottom": 7,
      "paddingBottom": 7
    },
    "children": [
      "flex-layout.col#product-image",
      "flex-layout.col#right-col"
    ]
  },
  "flex-layout.col#space": {
    "props": {
      "width": "10%"
    }
  },
  "flex-layout.col#product-image": {
    "props": {
      "width": "40%",
      "rowGap": 0
    },
    "children": [
      "product-images"
    ]
  },
  "product-images": {
    "props": {
      "displayThumbnailsArrows": true
    }
  },
  "flex-layout.col#right-col": {
    "props": {
      "preventVerticalStretch": true,
      "rowGap": 0
    },
    "children": [
      "product-name",
      "product-identifier.product",
      "product-separator",
      "product-price#product-details",
      "sku-selector",
      "product-quantity",
      "flex-layout.row#buy-button",
      "availability-subscriber",
      "share#default",
      "product-description"
    ]
  },
  "product-price#product-details": {
    "props": {
      "showInstallments": true,
      "showSavings": true,
      "blockClass": "productPrice"
    }
  },
  "flex-layout.row#buy-button": {
    "props": {
      "marginTop": 4,
      "marginBottom": 4,
      "width": "50%"
    },
    "children": [
      "flex-layout.col#buy-button"
    ]
  },
  "flex-layout.col#buy-button": {
    "children": [
      "buy-button#product"
    ],
    "props": {
      "width": "50%",
      "height": "150px"
    }
  },
  "buy-button#product": {
    "props": {
      "blockClass": "produto"
    }
  },
  "share#default": {
    "props": {
      "social": {
        "Facebook": true,
        "WhatsApp": true,
        "Twitter": false,
        "Pinterest": true
      }
    }
  }
}