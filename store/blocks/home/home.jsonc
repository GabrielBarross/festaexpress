      "shelf#shelf-two",
      "flex-layout.row#banner2",
      "shelf#shelf-three",
      "flex-layout.row#banner3",
      "shelf#shelf-four",
      "flex-layout.row#banner4",
      "shelf#shelf-five"
    ]
  },

  "carousel#home": {
    "props": {
      "autoplay": true,
      "banners": [
        {
          "image": "https://festaexpress.vteximg.com.br/arquivos/banner-liga.png",
          "mobileImage": "https://festaexpress.vteximg.com.br/arquivos/banner-liga.png"
        },
        {
          "image": "https://festaexpress.vteximg.com.br/arquivos/banner-lucas.png",
          "mobileImage": "https://festaexpress.vteximg.com.br/arquivos/banner-lucas.png"
        },
        {
          "image": "https://festaexpress.vteximg.com.br/arquivos/banner-festa-halloween.png",
          "mobileImage": "https://festaexpress.vteximg.com.br/arquivos/banner-festa-halloween.png"
        },
        {
          "image": "https://festaexpress.vteximg.com.br/arquivos/Banner-Principal-mes-das-criancas.png",
          "mobileImage": "https://festaexpress.vteximg.com.br/arquivos/Banner-Principal-mes-das-criancas.png"
        },
        {
          "image": "https://festaexpress.vteximg.com.br/arquivos/Banner-Principal-snoopy.png",
          "mobileImage": "https://festaexpress.vteximg.com.br/arquivos/Banner-Principal-snoopy.png"
        }
      ],
      "height": 400,
      "showArrows": true,
      "showDots": true
    }
  },

  "shelf#shelf-one": {
    "blocks": ["product-summary.shelf"],
    "props": {
      "orderBy": "OrderByTopSaleDESC",
      "paginationDotsVisibility": "desktopOnly",
      "productList": {
        "maxItems": 8,
        "itemsPerPage": 4,
        "minItemsPerPage": 2,
        "scroll": "BY_PAGE",
        "arrows": true,
        "titleText": "Festa Halloween"
      }
    }
  },
  "shelf#shelf-two": {
    "blocks": ["product-summary.shelf"],
    "props": {
      "orderBy": "OrderByTopSaleDESC",
      "paginationDotsVisibility": "desktopOnly",
      "productList": {
        "maxItems": 8,
        "itemsPerPage": 4,
        "minItemsPerPage": 2,
        "scroll": "BY_PAGE",
        "arrows": true,
        "titleText": "LIQUIDAFEST - Até 80% de desconto!"
      }
    }
  },
  "shelf#shelf-three": {
    "blocks": ["product-summary.shelf"],
    "props": {
      "orderBy": "OrderByTopSaleDESC",
