{
  "store.home": {
    "blocks": [
      "carousel#home",
      "flex-layout.row#deals",
      "shelf#home1",
      "flex-layout.row#banner1",
      "shelf#home2",
      "flex-layout.row#banner2",
      "shelf#home3",
      "flex-layout.row#banner3",
      "shelf#home4",
      "flex-layout.row#banner4",
      "shelf#home5",
      "newsletter"
